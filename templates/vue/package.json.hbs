{
  "name": "{{projectName}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint src --ext .js,.vue{{#if typescript}},.ts{{/if}}",
    "lint:fix": "eslint src --ext .js,.vue{{#if typescript}},.ts{{/if}} --fix"
  },
  "dependencies": {
    {{#if (eq vueVersion "3")}}
    "vue": "^3.2.47"
    {{else}}
    "vue": "^2.7.14"
    {{/if}}
    {{#if router}}
    {{#if (eq vueVersion "3")}}
    ,"vue-router": "^4.1.6"
    {{else}}
    ,"vue-router": "^3.6.5"
    {{/if}}
    {{/if}}
  },
  "devDependencies": {
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "html-webpack-plugin": "^5.5.0",
    {{#if (eq vueVersion "3")}}
    "vue-loader": "^17.0.1",
    {{else}}
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.7.14",
    {{/if}}
    "css-loader": "^6.7.1",
    "style-loader": "^3.3.1",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2"
    {{#if typescript}}
    ,"@vue/compiler-sfc": "^3.2.47",
    "typescript": "^4.8.4",
    "ts-loader": "^9.4.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@types/jest": "^29.2.3",
    "ts-jest": "^29.0.3"
    {{else}}
    ,"babel-loader": "^8.2.5",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    {{#if (eq vueVersion "3")}}
    "@vue/test-utils": "^2.2.6"
    {{else}}
    "@vue/test-utils": "^1.3.6"
    {{/if}}
    {{/if}}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}