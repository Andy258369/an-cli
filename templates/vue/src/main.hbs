{{#if (eq vueVersion "3")}}
import { createApp } from 'vue'
{{else}}
import Vue from 'vue'
{{/if}}
{{#if router}}
import router from './router'
{{/if}}
import App from './App.vue'
import './styles/main.{{#if typescript}}scss{{else}}css{{/if}}'

{{#if (eq vueVersion "3")}}
const app = createApp(App)

{{#if router}}
app.use(router)
{{/if}}

app.mount('#app')
{{else}}
Vue.config.productionTip = false

new Vue({
  {{#if router}}
  router,
  {{/if}}
  render: h => h(App),
}).$mount('#app')
{{/if}}